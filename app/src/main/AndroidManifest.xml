<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.santong.nock">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/nock_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activitys.HomeActivity"
            android:label="@string/app_name"
            android:launchMode="singleInstance">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".activitys.AddPlanActivity"
            android:label="@string/app_name" />

        <activity
            android:name=".activitys.PlanDetailActivity"
            android:label="@string/app_name" />
        <activity
            android:name=".activitys.PlanReportActivity"
            android:label="@string/app_name" />
    </application>

</manifest>
